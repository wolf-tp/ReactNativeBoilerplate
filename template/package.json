{
  "name": "BaseApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "splash": "yarn react-native generate-bootsplash splash/splash.png --background-color=FFFFFF --logo-width=200  --assets-path=assets --flavor=main",
    "android-dev": "node ./scripts/android.js env/.dev devDebug",
    "android-prod": "node ./scripts/android.js env/.prod prodDebug",
    "ios-dev": "node ./scripts/ios.js env/.dev",
    "ios-prod": "node ./scripts/ios.js env/.prod",
    "android": "react-native run-android",
    "generate": "node ./scripts/generator",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint  index.js ./__tests__/** ./scripts/*.js  ./src/** --ext .js,.ts,.tsx",
    "postinstall": "node ./scripts/setup.js",
    "ts-node": "ts-node ./scripts/generator-handle"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^1.8.3",
    "react": "18.0.0",
    "react-native": "0.69.3",
    "react-native-bootsplash": "^4.2.3",
    "react-native-config": "^1.4.6",
    "react-redux": "^8.0.2",
    "redux-saga": "^1.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/node": "^18.6.3",
    "@types/react-native": "^0.69.3",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^7.32.0",
    "eslint-plugin-import": "^2.26.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.70.3",
    "patch-package": "^6.4.7",
    "react-test-renderer": "18.0.0",
    "replace-in-file": "^6.3.5",
    "ts-node": "^10.9.1",
    "tslib": "^2.4.0",
    "typescript": "^4.4.4",
    "yargs": "^17.5.1"
  },
  "lint-staged": {
    "*.{ts,tsx,js}": [
      "eslint  --ext"
    ]
  },
  "resolutions": {
    "@types/react": "^18"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
